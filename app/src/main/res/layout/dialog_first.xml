<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/TextView_dialogFirst_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="이름"
        android:layout_marginLeft="@dimen/dialog_horizontal_margin"
        android:layout_marginTop="@dimen/dialog_vertical_margin"
        android:textSize="@dimen/dialog_textview_size"
        ></TextView>
    <EditText
        android:id="@+id/EditText_dialogFirst_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/TextView_dialogFirst_name"
        app:layout_constraintTop_toBottomOf="@id/TextView_dialogFirst_name"
        android:layout_marginHorizontal="@dimen/dialog_horizontal_margin"
        ></EditText>
    <TextView
        android:id="@+id/TextView_dialogFirst_phoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/TextView_dialogFirst_name"
        app:layout_constraintTop_toBottomOf="@id/EditText_dialogFirst_name"
        android:text="연락할 전화번호"
        android:layout_marginTop="@dimen/dialog_vertical_margin"
        android:textSize="@dimen/dialog_textview_size"
        ></TextView>
    <EditText
        android:id="@+id/EditText_dialogFirst_phoneNumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/TextView_dialogFirst_name"
        app:layout_constraintTop_toBottomOf="@id/TextView_dialogFirst_phoneNumber"
        android:layout_marginHorizontal="@dimen/dialog_horizontal_margin"
        android:inputType="phone"
        ></EditText>

</androidx.constraintlayout.widget.ConstraintLayout>